{
    "name": "electron-http-notifier",
    "version": "0.9.0",
    "description": "Light-weight tray application that allows you to show native desktop notification by http request.",
    "main": "main.js",
    "license": "MIT",
    "icon": "Icon.png",
    "scripts": {
        "start": "PORT=8008 HOST=127.0.0.1 ./node_modules/.bin/electron .",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "postinstall": "electron-builder install-app-deps",
        "test": "./node_modules/.bin/electron-mocha"
    },
    "author": "zentby",
    "build": {
        "appId": "io.github.zentby",
        "productName": "Http Notifier"
    },
    "devDependencies": {
        "electron": "^1.8.4",
        "electron-mocha": "^6.0.1",
        "electron-builder": "^20.8.1",
        "spectron": "^3.8.0"
    },
    "dependencies": {
    }
}